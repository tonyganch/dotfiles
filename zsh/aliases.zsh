alias src='source ~/.zshrc'
alias :q=exit

# Directories
alias ~='cd'
alias ~gh='cd ~/github'
alias ~tg='cd ~/github/tonyganch'
alias ~dot='cd ~/github/tonyganch/dotfiles'
alias ~sk='cd ~/github/sidekick'

# Key bindings
bindkey '^R' history-incremental-search-backward

# Sidekick
alias ~sidekick='/Applications/Sidekick.app/Contents/MacOS/Sidekick --disable-extension-guard-for-testing --enable-extension-activity-logging'
alias ~sideprod='/Applications/Sidekick\ Prod.app/Contents/MacOS/Sidekick --disable-extension-guard-for-testing --user-data-dir=/Users/tonyganch/sidekick/user-data'
alias ~sidedev='/Applications/Sidekick\ Dev.app/Contents/MacOS/Sidekick --disable-extension-guard-for-testing --user-data-dir=/Users/tonyganch/sidekick/user-data'
